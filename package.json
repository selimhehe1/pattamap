    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-query": "^5.90.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-ga4": "^2.1.0",
    "react-i18next": "^16.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.5.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:desktop": "playwright test --project=chromium-desktop",
    "test:e2e:mobile": "playwright test --project=chromium-mobile",
    "test:e2e:ci": "playwright test --project=chromium-desktop --reporter=list",
    "test:screenshots": "playwright test --grep @screenshot",
    "test:report": "playwright show-report tests/e2e/reports/html",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --max-warnings 100",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run build",
    "ci:full": "npm run ci && npm run test:e2e:ci"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@playwright/test": "^1.56.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.0.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "baseline-browser-mapping": "^2.9.11",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.3.0",
    "playwright": "^1.56.0",
    "purgecss": "^7.0.2",
    "sharp": "^0.34.5",
    "source-map-explorer": "^2.5.3",
    "to-ico": "^1.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.15",
    "webpack-bundle-analyzer": "^4.10.2",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  }
}
