# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here
REACT_APP_API_URL=http://localhost:5000

# Sentry Configuration (Error Monitoring)
# Create free account at https://sentry.io and get your DSN from project settings
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/your-project-id
REACT_APP_SENTRY_ENVIRONMENT=development
# ðŸŽ¯ RECOMMENDED: Enable performance monitoring in production
# Sample rate 0.5 (50%) provides good visibility without excessive quota
REACT_APP_SENTRY_ENABLE_TRACING=true
REACT_APP_SENTRY_TRACES_SAMPLE_RATE=0.5

# Google Analytics 4 (Phase 3A - Analytics & SEO)
# Create GA4 property at https://analytics.google.com and get Measurement ID
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# Optional: Enable GA4 in development mode for testing
REACT_APP_GA_DEBUG=false

# SEO Configuration (Phase 3A)
# Production site URL (used for canonical URLs and Open Graph)
REACT_APP_SITE_URL=https://pattamap.com

# Security Notes:
# - Never commit .env files to version control
# - ANON_KEY is safe for frontend use
# - Never expose SERVICE_KEY in frontend
# - Sentry DSN is safe for frontend (public key)
# - GA Measurement ID is safe for frontend (public key)